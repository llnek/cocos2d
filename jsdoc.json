{
  "opts" : {
    "encoding": "utf8",
    "recurse" : true,
    "destination": "/tmp/jout"
  },

  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },

  "source": {
    "includePattern": ".+\\.js(doc)?$",
    "XexcludePattern": "(^|\\/|\\\\)_"
  },

  "plugins": [],

  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }

}
